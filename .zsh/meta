m_[U]='Õ'
m_[V]='Ö'
m_[W]='÷'
m_[X]='Ø'
m_[Y]='Ù'
m_[Z]='Ú'
m_[\"]='¢'
m_[\']='§'
m_[\<]='¼'
m_[\>]='¾'
m_[\[]='Û'
m_[\\]='Ü'
m_[\]]='Ý'
m_[\`]='à'
m_[\{]='û'
m_[\|]='ü'
m_[\}]='ý'
m_[_]='ß'
